{
  "name": "Base",
  "version": "1.0.0",
  "description": "React, Redux, Sass and Webpack Starter Kit",
  "main": "webpack.config.js",
  "scripts": {
    "build": "webpack -p --progress",
    "client:serv": "webpack-dashboard -- webpack-dev-server",
    "serve:dev": "node node_modules/.bin/nodemon --exec 'node -r dotenv/config -r babel-core/register' src/server/scripts/server --watch src/server",
    "start": "node -r babel-core/register src/server/scripts/server",
    "test:lint": "eslint **/*.js"
  },
  "author": "Chris Limbrick",
  "license": "ISC",
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single",
        {
          "allowTemplateLiterals": true
        }
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "axios": "0.15.3",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "dotenv": "2.0.0",
    "es6-promise": "4.0.5",
    "eslint": "^3.4.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-proxy-middleware": "0.17.3",
    "node-sass": "^3.8.0",
    "nodemon": "1.11.0",
    "passport": "^0.3.2",
    "passport-google": "^0.3.0",
    "path": "0.12.7",
    "postcss-loader": "^0.9.1",
    "react": "^15.2.1",
    "react-css-modules": "^3.7.8",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-server": "^1.14.1",
    "yargs": "6.6.0"
  },
  "dependencies": {}
}
